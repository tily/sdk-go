{
  "partitions": [
    {
      "defaults": {
        "hostname": "{service}.{region}.{dnsSuffix}",
        "protocols": [
          "https"
        ],
        "signatureVersions": [
          "v4",
          "s3",
	  "n2"
        ]
      },
      "dnsSuffix": "api.cloud.nifty.com",
      "partition": "niftycloud",
      "partitionName": "NIFTYCloud Standard",
      "regionRegex": "^(us|jp)\\-\\w+\\-\\d+$",
      "regions": {
        "jp-east-1": {
          "description": "jp-east-1"
        },
        "jp-east-2": {
          "description": "jp-east-2"
        },
        "jp-west-1": {
          "description": "jp-west-1"
        }
      },
      "services": {
        "rdb": {
          "endpoints": {
            "jp-east-1": {},
            "jp-east-2": {}
          }
        },
        "storage": {
          "defaults": {
            "protocols": [
              "http",
              "https"
            ],
            "signatureVersions": [
              "s3"
            ]
          },
          "endpoints": {
            "jp-east-1": {
              "hostname": "ncss.nifty.com",
              "signatureVersions": [
                "s3"
              ]
            },
            "jp-west-1": {
              "hostname": "west-1-ncss.nifty.com",
              "signatureVersions": [
                "s3"
              ]
            }
          }
        },
        "objectstorage": {
          "defaults": {
            "protocols": [
              "http",
              "https"
            ],
            "signatureVersions": [
              "s3"
            ]
          },
          "endpoints": {
            "jp-east-2": {
              "hostname": "jp-east-2.os.cloud.nifty.com",
              "signatureVersions": [
                "s3"
              ]
            }
          }
        },
        "computing": {
          "defaults": {
            "protocols": [
              "https"
            ],
            "signatureVersions": [
              "n2"
            ]
          },
          "endpoints": {
            "jp-east-1": {
              "hostname": "east-1.cp.cloud.nifty.com",
              "signatureVersions": [
                "n2"
              ]
            },
            "jp-east-2": {
              "hostname": "east-2.cp.cloud.nifty.com",
              "signatureVersions": [
                "n2"
              ]
            }
          }
        },
        "email": {
          "endpoints": {
            "aws-global": {
              "credentialScope": {
                "region": "east-1"
              },
              "hostname": "ess.api.cloud.nifty.com",
              "signatureVersions": [
                "v4"
              ]
            }
          },
          "isRegionalized": false,
          "partitionEndpoint": "aws-global"
        },
        "mqtt": {
          "endpoints": {
            "aws-global": {
              "credentialScope": {
                "region": "east-1"
              },
              "hostname": "mqtt.api.cloud.nifty.com",
              "signatureVersions": [
                "v4"
              ]
            }
          },
          "isRegionalized": false,
          "partitionEndpoint": "aws-global"
        }
      }
    }
  ],
  "version": 3
}
